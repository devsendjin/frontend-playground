    "csso-webpack-plugin": "^2.0.0-beta.3",
    "terser-webpack-plugin": "^5.3.0",
    "mini-css-extract-plugin": "^2.4.6",
    "css-loader": "^6.5.1",
    "postcss-loader": "^6.2.1",
    "sass-loader": "^12.4.0",
    "ts-loader": "^9.2.6",
    "sass": "^1.45.2",
    "postcss-preset-env": "^7.2.0",
    "autoprefixer": "^10.4.1",
    "postcss-flexbugs-fixes": "^5.0.2",
