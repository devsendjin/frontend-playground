---
date: 2022-03-24
title: "Main page"
permalink: " /"
eleventyExcludeFromCollections: true
---

extends /playground

append config
  - config.contentAttributes = { class: 'content-front-page' }

append scripts
  +scripts(['index.js'])

append styles
  +styles([`${page.filePathStem}.css`])

mixin link(href, name)
  a(href=href)&attributes(attributes)= name

append content
  h1 #{_site.name}
  p P.S. >> open console
  +link('/', 'to main')(target='_blank', rel='noopener noreferrer nofollow')
